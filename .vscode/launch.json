	"configurations": [
		{
			"name": "Start Node-deployed, open in Edge",
			"type": "msedge",
			"request": "launch",
			"url": "http://localhost:30000",
			"webRoot": "${workspaceFolder}",
			"preLaunchTask": "run-node-foundry-deployed"
